    <div class="col-md-10 ps-4 pe-2">
        <div class="row" style="height:400px">
            <div class="col-md-4">
                <!-- Compressive Strength Card -->
                <div class="card card-default" style="height:385px">
                    <div class="card-header align-items-center">
                        <h2 class="">Compressive Strength</h2>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-6">
                                <p class="text-end " style="color: #894DD9">Knit Line</p>
                                <!-- compStrength1 -->
                                <div class="row">
                                    <div class="form-floating mb-2 col-9">
                                        <input type="number" class="form-control" id="compStrength1" placeholder="" value="@Model.gCompStrFG_1" onblur="GCompStrFG_LFClick('gCompStrFG_1', this.value)">
                                        <label for="compStrength1">Comp. Str-1</label>
                                    </div>

                                    <div class="custom-control custom-checkbox col-3 text-center">
                                        <input type="checkbox" class="custom-control-input" id="compStrength1Check" checked="@Model.gCompStrFGKnit_1" onchange="GCompStrFG2_LFClick('gCompStrFGKnit_1',this)">
