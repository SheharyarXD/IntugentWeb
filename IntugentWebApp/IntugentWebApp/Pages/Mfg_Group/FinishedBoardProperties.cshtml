           <div class="col-md-6">
                <!-- RValue Card -->
                <div class="card card-default" style="height:385px">
                    <div class="card-header align-items-center">
                        <h2 class="">R Value</h2>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-4">
                                <p class="text-center" style="color: #894DD9">k Factor</p>
                            </div>
                            <div class="col-2">
                                <p class="text-center" style="color: #894DD9">Knit Line</p>
                            </div>
                            <div class="col-6">
                                <p class="text-center" style="color: #894DD9">Test Time & Date</p>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-4">
                                <!-- rWest -->
                                <div class="form-floating mb-2">
                                    <input type="number" class="form-control" id="rWest" placeholder="" value="@Model.gkFactor_3" onblur="gkFactor_LFClick('gkFactor_3',this.value)">
                                    <label for="rWest">@Model.gLoc3B</label>
                                </div>
                            </div>
                            <div class="col-2 d-flex justify-content-center align-items-center">
                                <!-- rWestKnitLine -->
                                <div class="custom-control custom-checkbox col-3 text-center">
                                    <input type="checkbox" class="custom-control-input" id="rWestKnitLine" checked="@Model.gRValueKnitPresent3" onchange="CheckBoxes_LFClick('gRValueKnitPresent3',this)">
                                    <label class="custom-control-label" for="rWestKnitLine"> </label>
                                </div>
                            </div>
                            <div class="col-6">
                                <!-- rWestTestTime -->
                                <div class="form-floating mb-3">
                                    <input type="datetime-local" class="form-control text-center" id="rWestTestTime" value="@Model.gkFactorTime3?.ToString("yyyy-MM-ddTHH:mm")" onblur="GkFactorTime3_LF(this.value)">
                                    <label for="rWestTestTime">Test Time</label>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-4">
                                <!-- rCenter -->
                                <div class="form-floating mb-2">
                                    <input type="number" class="form-control" id="rCenter" placeholder="" value="@Model.gkFactor_2" onblur="gkFactor_LFClick('gkFactor_2' ,this.value)">
                                    <label for="rCenter">@Model.gLoc2B</label>
                                </div>
