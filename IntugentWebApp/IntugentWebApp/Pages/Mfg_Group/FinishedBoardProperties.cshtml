@page
@model IntugentWebApp.Pages.Mfg_Group.FinishedBoardPropertiesModel
@{
    ViewData["ActivePage"] = "Finished Board Properties";
}

<!-- Error message -->
@if (TempData["ErrorOnServer"] != null)
{
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        Error: @TempData["ErrorOnServer"]
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}

<div class="row ">
    <!-- General Info Card -->
    <div class="card card-default col-md-4" style="height:280px">
        <div class="card-header align-items-center">
            <h2 class="">General Info</h2>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-lg-6 d-flex align-items-center">
                    <!-- Dataset -->
                    <label class="col-4 col-form-label text-end">Dataset</label>
                    <div class="input-group col-8 d-flex">
                        <button class="btn btn-primary m-auto" type="button" style="padding: 9px" name="" onclick="gNavClick('prev')" @(Model.gDataSetPrev ? "" : "disabled")><span class="mdi mdi-chevron-left"></span></button>
                        <input type="number" class="form-control text-center w-50" disabled value="@Model.gID">
                        <button onclick="gNavClick('next')" @(Model.gDataSetNext ? "" : "disabled") class="btn btn-primary m-auto" type="button" style="padding: 9px"><span class="mdi mdi-chevron-right"></span></button>
                    </div>
                </div>

                <div class="col-md-3">
                    <!-- ipLength -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="ipLength" placeholder="" value="@Model.gIPLength" disabled />
                        <label for="ipLength">IP Length</label>
                    </div>
                </div>

                <div class="col-md-3">
                    <!-- ipWidth -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="ipWidth" placeholder="" value="@Model.gIPWidth" disabled />
                        <label for="ipWidth">IP Width</label>
                    </div>
                </div>

               
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- mfgDate -->
                    <div class="form-floating mb-2">
                        <input type="datetime-local" class="form-control" id="mfgDate" value="@Model.gTestDate" disabled>
                        <label for="mfgDate">Mfg. Date</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- productCode -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="productCode" placeholder="" value="@Model.gProdCode" disabled>
                        <label for="productCode">Product Code</label>
                    </div>
                </div>
            </div>
            <div class="row">
                

                <div class="col-md-5">
                    <!-- finishedBoardTimeStamp -->
                    <div class="form-floating mb-2">
                        <input type="datetime-local" class="form-control" id="finishedBoardTimeStamp" value="@Model.gFBTimeStamp?.ToString("yyyy-MM-ddTHH:mm")" onblur="GenInfoQC_Time_LF(this.value)">
                        <label for="finishedBoardTimeStamp">Finished Board Time Stamp</label>
                    </div>
                </div>

                <div class="col-md-3">
                    <!-- bundleHeight -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="bundleHeight" placeholder="" value="@Model.gBundleHeight" onblur="GBundleHeight_LF(this.value)">
                        <label for="bundleHeight">Bundle Height</label>
                    </div>
                </div>

                <div class="col-md-4">
                    <!-- passedQualityTesting -->
                    <div class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input" id="passedQualityTesting" checked="@Model.gTestingPassed" onchange="CheckBoxes_LFClick('gTestingPassed',this)">
                        <label class="custom-control-label" for="passedQualityTesting">Passed Quality Testing</label>
                    </div>
                    <!-- fgTestingComplete -->
                    <div class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input" id="fgTestingComplete" checked="@Model.gFinsihedGoodsDone" onchange="CheckBoxes_LFClick('gFinsihedGoodsDone',this)">
                        <label class="custom-control-label" for="fgTestingComplete">FG Testing Complete</label>
                    </div>
                </div>
            </div>

        </div>
    </div>
        <!-- qcTimes Card -->
    <div class="card card-default col-md-2" style="height:280px">
            <div class="card-header align-items-center">
                <h2 class="">QC Times</h2>
            </div>
            <div class="card-body my-3">
                <!-- timePourTable -->
                <div class="form-floating mb-2">
                    <input type="time" class="form-control text-center" id="timePourTable" value="@Model.gTimePourTableQC" disabled>
                    <label for="timePourTable">Time Pour Table QC Check</label>
                </div>
                <!-- inProcessBoardTimeStamp -->
                <div class="form-floating mb-2">
                    <input type="datetime-local" class="form-control text-center" id="inProcessBoardTimeStamp" value="@Model.gIPBoardTimeStamp" disabled>
                    <label for="inProcessBoardTimeStamp">In Process Board Time Stamp</label>
                </div>
                <!-- nextDayQCCollectionTime -->
                <div class="form-floating mb-2">
                    <input type="datetime-local" class="form-control text-center illegalValue" id="nextDayQCCollectionTime" value="@Model.gQCTimesDateTime?.ToString("yyyy-MM-ddTHH:mm")" onblur="GQCTimesDateTime_LF(this.value)" onfocus="">
                    <label for="nextDayQCCollectionTime">Next Day QC Collection Time</label>
                </div>
                <!-- ipTimeStampNotLegible -->
                <div class="custom-control custom-checkbox mb-2">
                    <input type="checkbox" class="custom-control-input" id="ipTimeStampNotLegible" checked="@Model.gIPTimeNotLegible" onchange="IPTimeNotLegible(this)">
                    <label class="custom-control-label" for="ipTimeStampNotLegible">If Checked - IP Time Stamp was NOT Legible</label>
                </div>
            </div>
        </div>
    <div class="col-md-3">
        <!-- qcSummary Card -->
        <div class="card card-default">
            <div class="card-header align-items-center">
                <h2 class="">QC Summary</h2>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-6">
                        <!-- fgCoreDensity -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="fgCoreDensity" placeholder="" value="@Model.gCoreDensity" disabled>
                            <label for="fgCoreDensity">FG Core Density</label>
                        </div>
                        <!-- avgCompStrength5 -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="avgCompStrength5" placeholder="" value="@Model.gCompStrFG_Avg5" disabled>
                            <label for="avgCompStrength5">Avg Comp Strength-5</label>
                        </div>
                        <!-- avgCompStrength6 -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="avgCompStrength6" placeholder="" value="@Model.gCompStrFG_Avg6" disabled>
                            <label for="avgCompStrength6">Avg Comp Strength-6</label>
                        </div>
                        <!-- fgThicknessAvg -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="fgThicknessAvg" placeholder="" value="@Model.gThickness" disabled>
                            <label for="fgThicknessAvg">Thickness Avg FG</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <!-- fgBoardProfile -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="fgBoardProfile" placeholder="" value="@Model.gFlatness" disabled>
                            <label for="fgBoardProfile">Board Profile FG</label>
                        </div>
                        <!-- initialRValueAvgQC -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="initialRValueAvgQC" placeholder="" value="@Model.gRValue" disabled>
                            <label for="initialRValueAvgQC">Initial R Value Avg</label>
                        </div>
                        <!-- facerPeelAvgQC -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center illegalValue" id="facerPeelAvgQC" placeholder="" value="@Model.gFacerPeelAvg_QC" disabled>
                            <label for="facerPeelAvgQC">Facer Peel Avg</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
      
    </div>
    <div class="col-md-3">
        <!-- ipQcSummary Card -->
        <div class="card card-default">
            <div class="card-header align-items-center">
                <h2 class="">IP QC Summary</h2>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-6">
                        <!-- warehouseTemp -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="warehouseTemp" placeholder="" value="@Model.gWarehouseTemp" disabled>
                            <label for="warehouseTemp">Warehouse Temp</label>
                        </div>
                        <!-- warehouseHumidity -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="warehouseHumidity" placeholder="" value="@Model.gWarehouseHumidity" disabled>
                            <label for="warehouseHumidity">Warehouse Humidity</label>
                        </div>
                        <!-- bundleHeightIP -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="bundleHeightIP" placeholder="" value="@Model.gBundleHeightIP" disabled>
                            <label for="bundleHeightIP">Bundle Height IP</label>
                        </div>
                        <!-- thicknessIP -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="thicknessIP" placeholder="" value="@Model.gThicknessIP" disabled>
                            <label for="thicknessIP">Thickness IP</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <!-- coreDensityIP -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="coreDensityIP" placeholder="" value="@Model.gThicknessIP" disabled>
                            <label for="coreDensityIP">Core Density IP</label>
                        </div>
                        <!-- avgCompStrength5IP -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="avgCompStrength5IP" placeholder="" value="@Model.gCompressiveIP5" disabled>
                            <label for="avgCompStrength5IP">Avg Comp Strength-5 IP</label>
                        </div>
                        <!-- avgCompStrength6IP -->
                        <div class="form-floating mb-2">
                            <input type="number" class="form-control text-center" id="avgCompStrength6IP" placeholder="" value="@Model.gCompressiveIP" disabled>
                            <label for="avgCompStrength6IP">Avg Comp Strength-6 IP</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


