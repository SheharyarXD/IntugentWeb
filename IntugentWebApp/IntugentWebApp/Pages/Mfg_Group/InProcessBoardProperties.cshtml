            $.ajax({
                type: "POST",
                url: "/Mfg_Group/InProcessBoardProperties?handler=NewDataset",
                headers: { "RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val() },
                data: { caller: caller },
