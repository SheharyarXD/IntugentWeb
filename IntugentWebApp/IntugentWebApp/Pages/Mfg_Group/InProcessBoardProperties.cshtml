                                <label for="compressiveIP3">Compressive IP-3</label>
                            </div>
                            <div class="custom-control custom-checkbox col-3 text-center">
                                <input type="checkbox" class="custom-control-input" id="compressiveIP3Check" asp-for="gCompStrKnitPresent_3IsChecked" checked="@Model.gCompStrKnitPresent_3IsChecked">
                                <label class="custom-control-label" for="compressiveIP3Check"> </label>
                            </div>
                        </div>
                        <!-- compressiveIP4 -->
                        <div class="row">
                            <div class="form-floating mb-1 col-9">
                                <input type="number" class="form-control" id="compressiveIP4" placeholder="" asp-for="gCompressiveIP_4Text" onblur="CompressiveStr_LF('gCompressiveIP_4', this.value)">
                                <label for="compressiveIP4">Compressive IP-4</label>
                            </div>
                            <div class="custom-control custom-checkbox col-3 text-center">
                                <input type="checkbox" class="custom-control-input" id="compressiveIP4Check" asp-for="gCompStrKnitPresent_4IsChecked" checked="@Model.gCompStrKnitPresent_4IsChecked">
