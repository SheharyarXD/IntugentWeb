                                <label for="compressiveIP2">Compressive IP-2</label>
                            </div>
                            <div class="custom-control custom-checkbox col-3 text-center">
                                <input type="checkbox" class="custom-control-input" id="compressiveIP2Check" asp-for="gCompStrKnitPresent_2IsChecked" checked="@Model.gCompStrKnitPresent_2IsChecked">
                                <label class="custom-control-label" for="compressiveIP2Check"> </label>
                            </div>
                        </div>
                        <!-- compressiveIP3 -->
