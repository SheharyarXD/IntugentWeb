                                <label for="compressiveIP6">Compressive IP-6</label>
                            </div>
                            <div class="custom-control custom-checkbox col-3 text-center">
                                <input type="checkbox" class="custom-control-input" id="compressiveIP6Check" asp-for="gCompStrKnitPresent_6IsChecked" checked="@Model.gCompStrKnitPresent_6IsChecked">
                                <label class="custom-control-label" for="compressiveIP6Check"> </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-10">
                <!-- General Info Card -->
                <div class="card card-default">
                    <div class="card-header align-items-center">
                        <h2 class="">General Info</h2>
                    </div>
                    <div class="card-body">
                        <form method="post">
                            <div class="row">
                                <div class="col-md-2">
                                    <!-- Dataset -->
                                    <div class="row">
                                        <label for="dataset" class="col-3 col-form-label text-end">Dataset</label>
                                        <div class="input-group col-9 d-flex align-items-center">
                                            <button class="btn btn-primary m-auto" type="button" style="padding: 9px; height: calc(1.5em + 0.9rem + 4px);" @(Model.gDataSetPrevIsEnabled ? "" : "disabled") onclick="NewDataset('gDataSetPrev')"><span class="mdi mdi-chevron-left"></span></button>
                                            <input type="number" class="form-control text-center w-50" asp-for="gID" disabled>
                                            <button class="btn btn-primary m-auto" type="button" style="padding: 9px; height: calc(1.5em + 0.9rem + 4px);" @(Model.gDataSetNextIsEnabled ? "" : "disabled") onclick="NewDataset('gDataSetNext')"><span class="mdi mdi-chevron-right"></span></button>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-2 ps-2">
                                    <!-- inProcessTestingComplete -->
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="inProcessTestingComplete" checked="@Model.gInProcessDoneIsChecked">
                                        <label class="custom-control-label" for="inProcessTestingComplete">In Process Testing Complete</label>
                                    </div>
                                    <!-- abandoned -->
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="abandoned" checked="@Model.gAbandonedIsChecked">
                                        <label class="custom-control-label" for="abandoned">Abandoned</label>
                                    </div>
                                </div>

                                <div class="col-md-2 d-flex justify-content-end m-sm-2 m-lg-1">
                                    <div class="row" style="margin: 0;">
                                        <button type="button" class="btn btn-primary m-auto">
                                            Create New Dataset
                                        </button>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2">
                                    <!-- Run Type -->
                                    <div class="form-floating mb-1">
                                        <select class="form-select" id="runType">
                                            @foreach (DataRowView row in Model.gRunType)
                                            {
                                                <option value="@row["ID"]" selected="@(Model.gRunTypeSelectedValue == (int)row["ID"])">@row["sName"]</option>
                                            }
                                        </select>
                                        <label for="runType">Run Type</label>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <!-- Product Code -->
                                    <div class="form-floating mb-1">
                                        <select class="form-select" id="productCode">
                                            @foreach (DataRowView row in Model.gProdID)
                                            {
                                                <option value="@row["Product Code"]" selected="@(Model.gProdIDSelectedValue == row["Product Code"].ToString())">@row["Product"]</option>
                                            }
                                        </select>
                                        <label for="productCode">Product Code</label>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <!-- paperManufacturer -->
                                    <div class="form-floating mb-1">
                                        <select class="form-select" id="paperManufacturer">
                                            @foreach (DataRowView row in Model.gPaperManufacturer)
                                            {
                                                <option value="@row["ID"]" selected="@(Model.gPaperManufacturerSelectedValue == (int)row["ID"])">@row["sName"]</option>
                                            }
                                        </select>
                                        <label for="paperManufacturer">Paper Manufacturer</label>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <!-- shift -->
                                    <div class="form-floating mb-1">
                                        <select class="form-select" id="shift">
                                            @foreach (DataRowView row in Model.gShift)
                                            {
                                                <option value="@row["ID"]" selected="@(Model.gShiftSelectedValue == (int)row["ID"])">@row["sName"]</option>
                                            }
                                        </select>
                                        <label for="shift">Shift</label>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <!-- operator -->
                                    <div class="form-floating mb-1">
                                        <select class="form-select" id="operator">
                                            @foreach (DataRowView row in Model.gOperator)
                                            {
                                                <option value="@row["ID"]" selected="@(Model.gOperatorSelectedItem == (int)row["ID"])">@row["Employees"]</option>
                                            }
                                        </select>
                                        <label for="operator">Operator</label>
                                    </div>
                                </div>

                            </div>

                            <div class="row">

                                <div class="col-md-2">
                                    <!-- runningWetDensity -->
                                    <div class="form-floating">
                                        <input type="number" class="form-control" id="runningWetDensity" placeholder="" asp-for="gRunningWetDesnsity">
                                        <label for="runningWetDensity">Running Wet Density</label>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <!-- warehouseTemp -->
                                    <div class="form-floating">
                                        <input type="number" class="form-control" id="warehouseTemp" placeholder="" asp-for="gWarehouseTemp">
                                        <label for="warehouseTemp">Warehouse Temp</label>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <!-- warehouseHumidity -->
                                    <div class="form-floating">
                                        <input type="number" class="form-control" id="warehouseHumidity" placeholder="" asp-for="gWarehouseHumidity">
                                        <label for="warehouseHumidity">Warehouse Humidity</label>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <!-- propTimeStamp -->
                                    <div class="form-floating">
                                        <input type="datetime-local" class="form-control" id="propTimeStamp" asp-for="gTestDateTime">
                                        <label for="propTimeStamp">Prop. Time Stamp</label>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <!-- checkTimestamp -->
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="checkTimestamp" asp-for="gTimeStampNotLegibleIsChecked" checked="@Model.gTimeStampNotLegibleIsChecked" )>
                                        <label class="custom-control-label" for="checkTimestamp">Check if TimeStamp NOT legible</label>
                                    </div>
                                </div>

                            </div>
                        </form>

                    </div>
                </div>

                <!-- Core Density Card -->
                <div class="card card-default">
                    <style>
                        .coreDensityTable td {
                            padding: 2px;
                        }
                    </style>
                    <div class="card-header align-items-center">
                        <h2 class="">Core Density</h2>
                    </div>
                    <div class="card-body">
                        <table class="table coreDensityTable">
                            <thead>
                                <tr class="text-center">
                                    <th></th>
                                    <th>Mass</th>
                                    <th>Length</th>
                                    <th>Width</th>
                                    <th colspan="5">Thickness</th>
                                    <th>Core Knit Present</th>
                                    <th>Density</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <th>@Model.gsLoc3A</th>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gMass_3Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control " asp-for="gL1_3Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control " asp-for="gW1_3Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT1_3Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT2_3Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT3_3Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT4_3Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT5_3Text">
                                        </div>
                                    </td>
                                    <td class="text-center">
                                        <label class="switch switch-primary switch-pill form-control-label ">
                                            <input type="checkbox" class="switch-input form-check-input" id="" asp-for="gCoreKnitPresent_3IsChecked" checked="@Model.gCoreKnitPresent_3IsChecked">
                                            <span class="switch-label"></span>
                                            <span class="switch-handle"></span>
                                        </label>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control text-center" asp-for="gCoreDensityIP_3Text" disabled>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <th>@Model.gsLoc2A</th>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gMass_2Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control " asp-for="gL1_2Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control " asp-for="gW1_2Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT1_2Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT2_2Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT3_2Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT4_2Text">
                                        </div>
                                    </td>
                                    <td>
                                        <div class="input-group input-group-sm ">
                                            <input type="number" class="form-control" asp-for="gT5_2Text">
                                        </div>
                                    </td>
