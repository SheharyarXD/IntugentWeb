    <div class="col-md-11 ps-3">
        <div class="row">
            <div class="col-md-2 ">
                <!-- Avg Values Card -->
                <div class="card card-default">
                    <div class="card-header align-items-center">
                        <h2 class="">Average Values</h2>
                    </div>
                    <div class="card-body">

                        <!-- coreDensityAvg -->
                        <div class="form-floating mb-1">
                            <input type="number" class="form-control" id="coreDensityAvg" placeholder="" asp-for="gCoreDensityIPText" disabled>
                            <label for="coreDensityAvg">Core Density Avg</label>
                        </div>
                        <!-- thicknessAvg -->
                        <div class="form-floating mb-1">
                            <input type="number" class="form-control" id="thicknessAvg" placeholder="" asp-for="gThicknessIPText" disabled>
                            <label for="thicknessAvg">Thickness Avg</label>
                        </div>
                        <!-- thicknessValley -->
                        <div class="form-floating mb-1">
                            <input type="number" class="form-control" id="thicknessValley" placeholder="" asp-for="gThicknessValleyText" disabled>
                            <label for="thicknessValley">Thickness Valley</label>
                        </div>
                        <!-- thicknessPeak -->
                        <div class="form-floating mb-1">
                            <input type="number" class="form-control" id="thicknessPeak" placeholder="" asp-for="gThicknessPeakText" disabled>
                            <label for="thicknessPeak">Thickness Peak</label>
                        </div>

                        <!-- boardProfile -->
                        <div class="form-floating mb-1">
                            <input type="number" class="form-control" id="boardProfile" placeholder="" asp-for="gFlatnessText" disabled>
                            <label for="boardProfile">Board Profile</label>
                        </div>
                        <!-- avgCompStrength5 -->
                        <div class="form-floating mb-1">
                            <input type="number" class="form-control" id="avgCompStrength5" placeholder="" asp-for="gCompressiveIP5Text" disabled>
                            <label for="avgCompStrength5">Avg Comp Strength-5</label>
                        </div>
                        <!-- avgCompStrength6 -->
                        <div class="form-floating mb-1">
                            <input type="number" class="form-control" id="avgCompStrength6" placeholder="" asp-for="gCompressiveIPText" disabled>
                            <label for="avgCompStrength6">Avg Comp Strength-6</label>
                        </div>

                    </div>
                </div>

                <!-- Compressive Strength Card -->
                <div class="card card-default">
                    <div class="card-header align-items-center">
                        <h2 class="">Compressive Strength</h2>
                    </div>
                    <div class="card-body">
                        <p class="text-end" style="color: #894DD9">Knit Line</p>
                        <!-- compressiveIP1 -->
                        <div class="row">
                            <div class="form-floating mb-1 col-9">
                                <input type="number" class="form-control" id="compressiveIP1" placeholder="" asp-for="gCompressiveIP_1Text" onblur="CompressiveStr_LF('gCompressiveIP_1', this.value)">
                                <label for="compressiveIP1">Compressive IP-1</label>
                            </div>

                            <div class="custom-control custom-checkbox col-3 text-center">
                                <input type="checkbox" class="custom-control-input" id="compressiveIP1Check" asp-for="gCompStrKnitPresent_1IsChecked" checked="@Model.gCompStrKnitPresent_1IsChecked">
                                <label class="custom-control-label" for="compressiveIP1Check"> </label>
                            </div>
                        </div>

                        <!-- compressiveIP2 -->
                        <div class="row">
                            <div class="form-floating mb-1 col-9">
