                <div class="card-body d-flex flex-column col-md-6 justify-content-evenly">
                    <!-- Bundle Quantity -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="BundleQuantity1" placeholder="" value="@Model.gQuantity" onblur="FirstBundle_LF('gQuantity', this.value)">
                        <label for="BundleQuantity1">Bundle Quantity</label>
                    </div>

                    <!-- Top Board Length -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="TopBoardLength1" placeholder="" value="@Model.gTopLength" onblur="FirstBundle_LF('gTopLength', this.value)">
                        <label for="TopBoardLength1">Top Board Length</label>
                    </div>
                    <!-- Bottom Board Length -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="BottomBoardLength1" placeholder="" value="@Model.gBottomLength" onblur="FirstBundle_LF('gBottomLength', this.value)">
                        <label for="BottomBoardLength1">Bottom Board Length</label>
                    </div>
                </div>
                <div class="card-body d-flex flex-column col-md-6 justify-content-evenly">
