                        <label for="LengthAverage">Length Average</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Length Average -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="LengthAverage_2" placeholder="" value="@Model.gLengthAvg_2" disabled>
                        <label for="LengthAverage_2">Length Average</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- Width -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Width" placeholder="" value="@Model.gWidthAvg_1" disabled>
                        <label for="Width">Width</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Width -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Width_2" placeholder="" value="@Model.gWidthAvg_2" disabled>
                        <label for="Width_2">Width</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- Squareness -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Squareness" placeholder="" value="@Model.gSquareness_1" disabled>
                        <label for="Squareness">Squareness</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Squareness -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Squareness2" placeholder="" value="@Model.gSquareness_2" disabled>
                        <label for="Squareness2">Squareness</label>
                    </div>
                </div>
