                </div>
            </div>
        </div>
    </div>
    <div class=" col-md-3">
        <!-- Second Bundle -->
        <div class="card card-default h-auto">
            <div class="card-header align-items-center">
                <h2 class="">Second Bundle</h2>
            </div>
            <div class="row px-4">
                <div class="card-body d-flex flex-column col-md-6 justify-content-evenly">
                    <!-- Bundle Quantity -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="BundleQuantity2" placeholder="" value="@Model.gQuantity_2" onblur="SecondBundle_LF('gQuantity_2', this.value)">
                        <label for="BundleQuantity2">Bundle Quantity</label>
                    </div>

                    <!-- Top Board Length -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="TopBoardLength2" placeholder="" value="@Model.gTopLength_2" onblur="SecondBundle_LF('gTopLength_2', this.value)">
                        <label for="TopBoardLength2">Top Board Length</label>
                    </div>
                    <!-- Bottom Board Length -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="BottomBoardLength2" placeholder="" value="@Model.gBottomLength_2" onblur="SecondBundle_LF('gBottomLength_2', this.value)">
                        <label for="BottomBoardLength2">Bottom Board Length</label>
                    </div>
                </div>
                <div class="card-body d-flex flex-column col-md-6 justify-content-evenly">
                    <!-- Board/Bundle Width -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="Board/BundleWidth2" placeholder="" value="@Model.gWidth_2" onblur="SecondBundle_LF('gWidth_2', this.value)">
                        <label for="Board/BundleWidth2">Board/Bundle Width</label>
                    </div>
                    <!-- Middle Board Length -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="MiddleBoardLength2" placeholder="" value="@Model.gMiddleLength_2" onblur="SecondBundle_LF('gMiddleLength_2', this.value)">
                        <label for="MiddleBoardLength2">Middle Board Length</label>
                    </div>
                    <!-- Diagonal 1 -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="Diagonal1_2" placeholder="" value="@Model.gDiagonal1_2" onblur="SecondBundle_LF('gDiagonal1_2', this.value)">
                        <label for="Diagonal1_2">Diagonal 1</label>
                    </div>
                </div>
            </div>
            <div class="row px-4 mb-2">
                <!-- Diagonal 2 -->
                <div class="form-floating">
                    <input type="number" class="form-control" id="Diagonal2_2" placeholder="" value="@Model.gDiagonal2_2" onblur="SecondBundle_LF('gDiagonal2_2', this.value)">
                    <label for="Diagonal2_2">Diagonal 2</label>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card card-default h-auto px-4">
            <div class="card-header align-items-center">
                <h2 class="">Board Deviation</h2>
            </div>
           <div class="card-body d-flex flex-column col-md-12 justify-content-evenly">
           <div class="col-md-12">
            <!-- Deviation from Table -->
            <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="DeviationfromTable" placeholder="" value="@Model.gDeviationAbs" onblur="Deviation_LF('gDeviationAbs', this.value)">
            <label for="DeviationfromTable">Deviation from Table</label>
            </div>
        </div>

               <div class="col-md-12">
            <!-- Deviation (Bowing) Type -->
            <div class="form-floating mb-2" onblur="GWF_Info_LF('gDevType', this.value)">
                        <select class="form-select" id="DeviationType" asp-for="gDeviationType" onblur="Deviation_LF('gDeviationType', this.value)">
                    <option value="Up">Up</option>
                    <option value="Down">Down</option>
                </select>
            <label for="DeviationType">Deviation Type</label>

            </div>
        </div>
            <div class="col-md-12">
                <!-- Deviation Directed -->
                <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Deviation" placeholder="" value="@Model.gBoardDeviationRel" disabled>
                    <label for="Deviation">Deviation (Directed)</label>
                </div>
            </div>
        </div>
    </div>

    </div>
    <div class="col-3 align-items-center mt-5">
        <div class="col-12 row align-items-center px-5 my-3">
            <button type="button" onclick=GCopyDataSet() class="btn btn-primary btn-sm">Copy Data To ClipBoard</button>
            </div>
            <div class="px-3">
            <!-- gCopyData -->
            <div class="form-floating mb-2" onblur="">
                <select class="form-select" id="CopyData" onblur="@Model.gCopyData==this.selectedIndex" >
                    <option value="today">Today's Data</option>
                    <option value="24hours">Past 24 hours</option>
