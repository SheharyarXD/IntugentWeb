                    <option value="1month">Past 1 month</option>
                    <option value="6months">Past 6 months</option>
                    <option value="1year">Past 1 year</option>
                    <option value="all">All Data</option>
                </select>
                <label for="CopyData">Copy Data</label>
            </div>
        </div>

    </div>
</div>
<div class="row">
    <!-- Board Thickness -->
    <div class="card card-default col-md-2">
        <div class="card-header align-items-center">
            <h2 class="">Board Thickness</h2>
        </div>
        <div class="card-body">
            <div class="row mt-3">
                <div class="col-md-6">
