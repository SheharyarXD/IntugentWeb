                    </div>
                    <!-- Diagonal 1 -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="Diagonal1_1" placeholder="" value="@Model.gDiagonal1" onblur="FirstBundle_LF('gDiagonal1', this.value)">
