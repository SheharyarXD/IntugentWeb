@page
@using System.Data;
@model IntugentWebApp.Pages.Mfg_Group.MfgProcessCheckModel
@{
    ViewData["ActivePage"] = "Process Check";
}
@* Error Message *@
@if (TempData["ErrorOnServer"] != null)
{
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        Error: @TempData["ErrorOnServer"]
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}
<div class="row">
    <div class="row col-12">
        @* DataSet Card *@
        <div class="card card-default col-2">
            <div class="card-header align-items-center">
                <h2 class="">Navigation</h2>
            </div>
            <div class="card-body">
                <!-- Dataset -->
                <label>Check Sheet</label>
                <div class="input-group col-12 d-flex">
                    <button class="btn btn-primary m-auto" type="button" style="padding: 9px" onclick="gNavClick('prev')"><span class="mdi mdi-chevron-left"></span></button>
                    <input type="number" class="form-control text-center w-50" disabled value="@Model.gID">
                    <button onclick="gNavClick('next')" name="" class="btn btn-primary m-auto" type="button" style="padding: 9px"><span class="mdi mdi-chevron-right"></span></button>
                    </div>
                </div>
            </div>
    <!-- General Info Card -->
    <div class="card card-default col-10">
        <div class="card-header align-items-center">
            <h2 class="">General Info</h2>
        </div>
        <div class="card-body">
                <div class="row align-items-center">
                <div class="col-3">
                    <!-- Production Code -->
                    <div class="form-floating mb-2" onblur="">
                            <select class="form-select" id="ProdID" onblur="GComboBox_LF('gProdID', this.value)">
                                @foreach (DataRowView row in Model.gProdID)
                                {
                                    <option value="@row["Product Code"]" selected="@(Model.gProdIDSelected == row["Product Code"].ToString())">@row["Product"]</option>
                                }
                        </select>
                            <label for="ProdID">Production Code</label>
                    </div>
                </div>
                <div class="col-2">
                    <!-- Date -->
                    <div class="form-floating mb-2">
                            <input type="datetime-local" class="form-control" id="MfgTimeStamp" value="@Model.gTestDate?.ToString("yyyy-MM-ddThh:mm")" onblur="GMisc_LF('gTestDate',this.value)">
                        <label for="MfgTimeStamp">Date</label>
                    </div>
                </div>
                    <div class="col-2">
                        <!-- Time Code -->
                        <div class="form-floating mb-2">
                            <input type="time" class="form-control" id="MfgTimeCode" value="@Model.gTestTime?.ToString("HH:mm:ss")" onblur="GTestTime_LF('gTestTime',this.value)">
                            <label for="MfgTimeCode">Time Code</label>
                        </div>
                    </div>
                    <div class="col-3">
                        <!-- Operator -->
                        <div class="form-floating mb-2" onblur="">
                            <select class="form-select" id="operator" onblur="GComboBox_LF('gOperator', this.value)">
                                @foreach (DataRowView row in Model.gOperator)
                                {
                                    <option value="@row["ID"]" selected="@(Model.gOperatorSelectedItem == (int)row["ID"])">@row["Employees"]</option>
                                }
                            </select>
                            <label for="operator">Operator</label>
                        </div>
                    </div>
                    <div class="col-2">
                        <!-- Type -->
                        <div class="form-floating mb-2" >
                            <select class="form-select" id="TypeOp" onblur="GComboBox_LF('gType', this.value)">
                                @foreach (DataRowView row in Model.gType)
                                            {
                                        <option value="@row["ID"]" selected="@(Model.gTypeSelectedValue == (int)row["ID"])">@row["sName"]</option>
                                            }
                            </select>
                            <label for="TypeOp">Type</label>
                        </div>
                    </div>
            </div>

        </div>
    </div>
    </div>
</div>

<div class="row">
 <div class="col-3">
    <div class="row col-12 my-3">
            <button type="button" onclick=gNewCheckSheet_Click() class="btn btn-primary btn-sm">New In Process Check</button>
            <!-- Exclude -->
            <div class="custom-control custom-checkbox align-items-center">
                <input type="checkbox" class="custom-control-input" id="Exclude" checked="@Model.gExcludeIsChecked" onchange=" GMisc_LFs('gExclude', this)">
                <label class="custom-control-label" for="Exclude">Do not Include in SPC Analyses</label>
            </div>
        </div>
    <!-- Print / Perferation / Flipper -->
    <div class="card card-default col-12">
        <div class="card-header align-items-center">
            <h2 class="">Print / Perferation / Flipper</h2>
        </div>
        <div class="card-body">
            <div class="row align-items-center">
                    <!-- Top board print -->
                    <div class="form-floating mb-2 col-6" onblur="">
                        <select class="form-select" id="TopBoardPrint" onblur="GComboBox_LF('gTopBoardPrint', this.value)">
                            @foreach (DataRowView row in Model.gTopBoardPrintCollection)
                            {
                     <option value="@row["ID"]" selected="@(Model.gTopBoardPrint == (int)row["ID"])">@row["sName"]</option>
                            }
                        </select>
                        <label for="TopBoardPrint">Top board print</label>
                    </div>
                    <!-- Bottom board print -->
                <div class="form-floating mb-2 col-6" onblur="">
                        <select class="form-select" id="BottomBoardPrint" onblur="GComboBox_LF('gBottomBoardPrint', this.value)">
                            @foreach (DataRowView row in Model.gBottomBoardPrintCol)
                            {
                                <option value="@row["ID"]" selected="@(Model.gBottomBoardPrint == (int)row["ID"])">@row["sName"]</option>
                            }
                    </select>
                        <label for="BottomBoardPrint">Bottom board print</label>
                </div>
            </div>
            <div class="row align-items-center">
                    <!-- Perferation -->
                <div class="form-floating mb-2 col-6" onblur="">
                        <select class="form-select" id="Perferation" onblur="GComboBox_LF('gPerferation', this.value)">
                            @foreach (DataRowView row in Model.gPerferationCol)
                            {
                                <option value="@row["ID"]" selected="@(Model.gPerferation == (int)row["ID"])">@row["sName"]</option>
                            }
                    </select>
                        <label for="Perferation">Perferation</label>
                </div>
                    <!-- Flipper Operating-->
                <div class="form-floating mb-2 col-6" onblur="">
                        <select class="form-select" id="FlipperOperating" onblur="GComboBox_LF('gFlipper', this.value)">
                            @foreach (DataRowView row in Model.gFlipperCol)
                            {
                                <option value="@row["ID"]" selected="@(Model.gFlipper == (int)row["ID"])">@row["sName"]</option>
                            }
                    </select>
                        <label for="FlipperOperating">Flipper Operating</label>
                </div>
            </div>
        </div>
    </div>
  </div>
    <!-- Facer, Foam, and Hooder -->
    <div class="card card-default col-4">
        <div class="card-header align-items-center">
            <h2 class="">Facer, Foam, and Hooder</h2>
        </div>
        <div class="card-body">
            <div class="row align-items-center">
                <!-- gAdhesion -->
                <div class="form-floating mb-2 col-6" onblur="">
                    <select class="form-select" id="gAdhesion" onblur="GComboBox_LF('gAdhesion', this.value)">
                        @foreach (DataRowView row in Model.gAdhesionCol)
                        {
                            <option value="@row["ID"]" selected="@(Model.gAdhesionSelected == (int)row["ID"])">@row["sName"]</option>
                        }
                    </select>
                    <label for="gAdhesion">Facer Adhesion</label>
                </div>
                <!-- Edge Cut -->
                <div class="form-floating mb-2 col-6" onblur="">
                    <select class="form-select" id="EdgeCut" onblur="GComboBox_LF('gEdgeCut', this.value)">
                        @foreach (DataRowView row in Model.gEdgeCutCol)
                        {
                            <option value="@row["ID"]" selected="@(Model.gEdgeCutSelected == (int)row["ID"])">@row["sName"]</option>
                        }
                    </select>
                    <label for="EdgeCut">Edge Cut</label>
                </div>
            </div>
               <!-- Exposed Foam -->
                <div class="form-floating my-2 col-12">
                <input type="number" class="form-control text-center" id="ExposedForm" placeholder="" value="@Model.gExposedFoam" onblur="GMisc_LF('gExposedFoam',this.value)">
                    <label for="ExposedForm">Exposed Foam</label>
                </div>
            <div class="row align-items-center">
                <!-- Hooder Quality -->
                <div class="form-floating mb-2 col-6" onblur="">
                    <select class="form-select" id="HooderQuality" onblur="GComboBox_LF('gHooder', this.value)">
                        @foreach (DataRowView row in Model.gHooderCol)
                        {
                            <option value="@row["ID"]" selected="@(Model.gHooderSelected == (int)row["ID"])">@row["sName"]</option>
                        }
                    </select>
                    <label for="HooderQuality">Hooder Quality</label>
                </div>
                <!-- Board Quality -->
                <div class="form-floating mb-2 col-6" onblur="">
                    <select class="form-select" id="BoardQuality" onblur="GComboBox_LF('gBoardQuality', this.value)">
                        @foreach (DataRowView row in Model.gBoardQualityCol)
                        {
                            <option value="@row["ID"]" selected="@(Model.gBoardQualitySelected == (int)row["ID"])">@row["sName"]</option>
                        }
                    </select>
                    <label for="BoardQuality">Board Quality</label>
                </div>
            </div>
        </div>
    </div>
    <!-- Cup Shot (Reactivity Data) -->
    <div class="card card-default col-md-3">
        <div class="card-header align-items-center">
            <h2 class="">Cup Shot (Reactivity Data)</h2>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-md-6">
                    <!-- Empty cup mass (g) -->
                    <div class="form-floating mb-2">
                        <input type="number" class="form-control" id="Emptycupmass" placeholder="" value="@Model.gEmptyCupMassG" onblur="GCupReactivity_LF('gEmptyCupMassG',this.value)">
                        <label for="Emptycupmass">Empty cup mass (g)</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Cream Time (s) -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="CreamTime" placeholder="" value="@Model.gCreamTimeS" onblur="GCupReactivity_LF('gCreamTimeS',this.value)">
                        <label for="CreamTime">Cream Time (s)</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- Gel Time (s) -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="GelTime" placeholder="" value="@Model.gGelTimeS" onblur="GCupReactivity_LF('gGelTimeS',this.value)">
                        <label for="GelTime">Gel Time (s)</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Tack-free Time (s) -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Tack-free" placeholder="" value="@Model.gTackFreeTimeS" onblur="GCupReactivity_LF('gTackFreeTimeS',this.value)">
                        <label for="Tack-free">Tack-free Time (s)</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- Full cup mass (g) -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Fullcupmass" placeholder="" value="@Model.gFullCupMassG" onblur="GCupReactivity_LF('gFullCupMassG',this.value)">
                        <label for="Fullcupmass">Full cup mass (g)r</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Foam Density (lb/ft3) -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="FoamDensity" placeholder="" value="@Model.gFoamDensityPCF" disabled>
                        <label for="FoamDensity">Foam Density (lb/ft3)</label>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-2 h-50">
        <!-- Comments -->
        <div class="form-floating ">
            <textarea class="form-control" placeholder="Address" id="Comment" style="height: 240px;" onblur="GMisc_LF('gComment',this.value)">@Model.gComment</textarea>
            <label for="Comment">Comments</label>
        </div>
    </div>
</div>


<div class="row">
    <div class=" col-md-3">
        <!-- First Bundle -->
        <div class="card card-default h-auto">
            <div class="card-header align-items-center">
                <h2 class="">First Bundle</h2>
            </div>
